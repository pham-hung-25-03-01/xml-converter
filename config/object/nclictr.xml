<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Application>
    <RegNumber type="default" use="required">CLI#{REG_NUMBER}*{NUMBER}</RegNumber>
    <OrderDprt>*{ORDER_DPRT}</OrderDprt>
    <ObjectType>Client</ObjectType>
    <ActionType>Add</ActionType>
    <Data>
        <Client>
            <ClientType>PR</ClientType>
            <ClientInfo>
                <RegNumberType>${OLD_ID_NUMBER}</RegNumberType>
                <RegNumber>${ID_NUMBER}</RegNumber>
                <RegNumberDetails>220720</RegNumberDetails>
                <SocialNumber>${FAMILY_BOOK_NUMBER}</SocialNumber>
                <ShortName>${CLIENT_NAME}</ShortName>
                <TaxpayerIdentifier>${TAX_ID}</TaxpayerIdentifier>
                <TaxPosition>${INCOME}</TaxPosition>
                <MiddleName>${SPOUSE_TAX_ID}</MiddleName>
                <SecurityName>${SPOUSE_NAME}</SecurityName>
                <Citizenship>${NATIONALITY}</Citizenship>
                <MaritalStatus>${MARITAL_STATUS}</MaritalStatus>
                <Position>${OCCUPATION}</Position>
                <CompanyName>${COMPANY_NAME}</CompanyName>
                <CompanyDepartment>${DIVISION}</CompanyDepartment>
                <BirthDate>${DOB}</BirthDate>
                <Gender>${SEX}</Gender>
            </ClientInfo>
            <PlasticInfo>
                <FirstName>${EMBOSSING_FIRST_NAME}</FirstName>
                <LastName>${EMBOSSING_LAST_NAME}</LastName>
            </PlasticInfo>
            <PhoneList>
                <Phone ref="COMPANY_PHONE;">
                    <PhoneType>Work</PhoneType>
                    <PhoneNumber type="number" use="default">${COMPANY_PHONE}</PhoneNumber>
                </Phone>
                <Phone ref="PHONE;">
                    <PhoneType>Home</PhoneType>
                    <PhoneNumber>${PHONE}</PhoneNumber>
                </Phone>
                <Phone ref="MOBILE_PHONE;">
                    <PhoneType>Mobile</PhoneType>
                    <PhoneNumber ref="MOBILE_PHONE;" type="number" use="required">${MOBILE_PHONE}</PhoneNumber>
                </Phone>
            </PhoneList>
            <DateOpen>${OPEN_DATE}</DateOpen>
            <BaseAddress>
                <AddressType>PIN</AddressType>
                <Email>${EMAIL}</Email>
                <City>${PIN_CITY}</City>
                <AddressLine1>${RESIDENT_ADDR_L1}</AddressLine1>
                <AddressLine2>${RESIDENT_ADDR_L2}</AddressLine2>
                <AddressLine3>${RESIDENT_ADDR_L3}</AddressLine3>
                <AddressLine4>${RESIDENT_ADDR_L4}</AddressLine4>
            </BaseAddress>
            <AddInfo>
                <AddInfo01>${NO_OF_DEPENDANTS}</AddInfo01>
                <AddInfo02>RES_PERIOD=23;OCC_PERIOD=2;DEPEND_NO=2;SP_ID_CARD=5555555;SP_ID_DATE=22/11/2021;</AddInfo02>
                <AddInfo03>${TYPE_OF_RESIDENCE}</AddInfo03>
            </AddInfo>
        </Client>
    </Data>
    <AddData>
        <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
    </AddData>
    <SubApplList>
        <Application>
            <RegNumber>ADD#{REG_NUMBER}*{NUMBER}00001</RegNumber>
            <OrderDprt>*{ORDER_DPRT}</OrderDprt>
            <ObjectType>ClientAddress</ObjectType>
            <ActionType>Add</ActionType>
            <Data>
                <Address>
                    <AddressType>PHS_ADDR</AddressType>
                    <City>${PIN_CITY}</City>
                    <AddressLine1>${CORRESPONDENT_ADDR_L1}</AddressLine1>
                    <AddressLine2>${CORRESPONDENT_ADDR_L2}</AddressLine2>
                    <AddressLine3>${CORRESPONDENT_ADDR_L3}</AddressLine3>
                    <AddressLine4>${CORRESPONDENT_ADDR_L4}</AddressLine4>
                </Address>
            </Data>
            <AddData>
                <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
            </AddData>
        </Application>
        <Application>
            <RegNumber>ADD#{REG_NUMBER}*{NUMBER}00002</RegNumber>
            <OrderDprt>*{ORDER_DPRT}</OrderDprt>
            <ObjectType>ClientAddress</ObjectType>
            <ActionType>Add</ActionType>
            <Data>
                <Address>
                    <AddressType>PST_ADDR</AddressType>
                    <City>${PIN_CITY}</City>
                    <AddressLine1>${PIN_ADDR_L1}</AddressLine1>
                    <AddressLine2>${PIN_ADDR_L2}</AddressLine2>
                    <AddressLine3>${PIN_ADDR_L3}</AddressLine3>
                    <AddressLine4>${PIN_ADDR_L4}</AddressLine4>
                </Address>
            </Data>
            <AddData>
                <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
            </AddData>
        </Application>
        <Application>
            <RegNumber>ADD#{REG_NUMBER}*{NUMBER}00003</RegNumber>
            <OrderDprt>*{ORDER_DPRT}</OrderDprt>
            <ObjectType>ClientAddress</ObjectType>
            <ActionType>Add</ActionType>
            <Data>
                <Address>
                    <AddressType>WRK_ADDR</AddressType>
                    <City>${PIN_CITY}</City>
                    <AddressLine1>${COMPANY_ADDR_L1}</AddressLine1>
                    <AddressLine2>${COMPANY_ADDR_L2}</AddressLine2>
                    <AddressLine3>${COMPANY_ADDR_L3}</AddressLine3>
                    <AddressLine4>${COMPANY_ADDR_L4}</AddressLine4>
                </Address>
            </Data>
            <AddData>
                <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
            </AddData>
        </Application>
        <Application>
            <RegNumber>LIB#{REG_NUMBER}*{NUMBER}</RegNumber>
            <OrderDprt>*{ORDER_DPRT}</OrderDprt>
            <ObjectType>Contract</ObjectType>
            <ActionType>Add</ActionType>
            <Data>
                <Contract>
                    <Product>
                        <ProductCode1>P001L</ProductCode1>
                    </Product>
                </Contract>
            </Data>
            <AddData>
                <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
            </AddData>
            <SubApplList>
                <Application>
                    <RegNumber>ISS#{REG_NUMBER}*{NUMBER}</RegNumber>
                    <OrderDprt>*{ORDER_DPRT}</OrderDprt>
                    <ObjectType>Contract</ObjectType>
                    <ActionType>Add</ActionType>
                    <Data>
                        <Contract>
                            <Product>
                                <ProductCode1>P04LI</ProductCode1>
                            </Product>
                        </Contract>
                    </Data>
                    <AddData>
                        <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
                    </AddData>
                    <SubApplList>
                        <Application>
                            <RegNumber>CLS#{REG_NUMBER}*{NUMBER}</RegNumber>
                            <OrderDprt>*{ORDER_DPRT}</OrderDprt>
                            <ObjectType>ContractClassifier</ObjectType>
                            <ActionType>AddOrUpdate</ActionType>
                            <Data>
                                <CustomerClassifiers>
                                    <Classifier>
                                        <Code>BILL_CYCLE</Code>
                                        <Value>${DUE_DATE}</Value>
                                    </Classifier>
                                </CustomerClassifiers>
                            </Data>
                            <AddData>
                                <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
                            </AddData>
                        </Application>
                        <Application>
                            <RegNumber>CRD#{REG_NUMBER}*{NUMBER}</RegNumber>
                            <OrderDprt>*{ORDER_DPRT}</OrderDprt>
                            <ObjectType>Contract</ObjectType>
                            <ActionType>Add</ActionType>
                            <Data>
                                <Contract>
                                    <Product>
                                        <ProductCode1>P04LIM</ProductCode1>
                                    </Product>
                                    <AddContractInfo>
                                        <AddInfo01>${CARD_MAILER_NUMBER}</AddInfo01>
                                        <AddInfo02>${PIN_MAILER_NUMBER}</AddInfo02>
                                    </AddContractInfo>
                                </Contract>
                            </Data>
                            <AddData>
                                <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
                            </AddData>
                            <SubApplList>
                                <Application>
                                    <RegNumber>EVT#{REG_NUMBER}*{NUMBER}</RegNumber>
                                    <OrderDprt>*{ORDER_DPRT}</OrderDprt>
                                    <ObjectType>Event</ObjectType>
                                    <ActionType>Add</ActionType>
                                    <Data>
                                        <QueEvent>
                                            <ActionCode>ON_ECOM</ActionCode>
                                            <Comment>Turn on ECOM</Comment>
                                        </QueEvent>
                                    </Data>
                                    <AddData>
                                        <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
                                    </AddData>
                                </Application>
                                <Application>
                                    <RegNumber>TRF#{REG_NUMBER}*{NUMBER}00001</RegNumber>
                                    <OrderDprt>*{ORDER_DPRT}</OrderDprt>
                                    <ObjectType>Tariff</ObjectType>
                                    <ActionType>Add</ActionType>
                                    <Data>
                                        <Tariffs>
                                            <Tariff>
                                                <UsageTariff>
                                                    <TariffType>
                                                        <TariffCode>ECOM_USG_DLY_NMBR_LMT</TariffCode>
                                                    </TariffType>
                                                    <ApplyMode>Active</ApplyMode>
                                                    <DateFrom>2023-04-27</DateFrom>
                                                    <MaxAmount>${ECOM_DAILY_LIMIT}</MaxAmount>
                                                    <Currency>VND</Currency>
                                                </UsageTariff>
                                            </Tariff>
                                        </Tariffs>
                                    </Data>
                                    <AddData>
                                        <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
                                    </AddData>
                                </Application>
                                <Application>
                                    <RegNumber>TRF#{REG_NUMBER}*{NUMBER}00002</RegNumber>
                                    <OrderDprt>*{ORDER_DPRT}</OrderDprt>
                                    <ObjectType>Tariff</ObjectType>
                                    <ActionType>Add</ActionType>
                                    <Data>
                                        <Tariffs>
                                            <Tariff>
                                                <UsageTariff>
                                                    <TariffType>
                                                        <TariffCode>ATM_USG_DLY_NMBR_LMT</TariffCode>
                                                    </TariffType>
                                                    <ApplyMode>Active</ApplyMode>
                                                    <DateFrom>2023-04-27</DateFrom>
                                                    <MaxAmount>${CASH_DAILY_LIMIT}</MaxAmount>
                                                    <Currency>VND</Currency>
                                                </UsageTariff>
                                            </Tariff>
                                        </Tariffs>
                                    </Data>
                                    <AddData>
                                        <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
                                    </AddData>
                                </Application>
                                <Application>
                                    <RegNumber>TRFTRF#{REG_NUMBER}*{NUMBER}00003</RegNumber>
                                    <OrderDprt>*{ORDER_DPRT}</OrderDprt>
                                    <ObjectType>Tariff</ObjectType>
                                    <ActionType>Add</ActionType>
                                    <Data>
                                        <Tariffs>
                                            <Tariff>
                                                <UsageTariff>
                                                    <TariffType>
                                                        <TariffCode>POS_USG_DLY_NMBR_LMT</TariffCode>
                                                    </TariffType>
                                                    <ApplyMode>Active</ApplyMode>
                                                    <DateFrom>2023-04-27</DateFrom>
                                                    <MaxAmount>${RETAIL_DAILY_LIMIT}</MaxAmount>
                                                    <Currency>VND</Currency>
                                                </UsageTariff>
                                            </Tariff>
                                        </Tariffs>
                                    </Data>
                                    <AddData>
                                        <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
                                    </AddData>
                                </Application>
                            </SubApplList>
                        </Application>
                    </SubApplList>
                </Application>
            </SubApplList>
        </Application>
    </SubApplList>
</Application>
