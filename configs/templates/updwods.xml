<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<Application>
    <RegNumber>UWD#{REG_NUMBER}*{NUMBER}</RegNumber>
    <OrderDprt>*{ORDER_DPRT}</OrderDprt>
    <ObjectType>Event</ObjectType>
    <ActionType>Add</ActionType>
    <ObjectFor>
        <ContractIDT>
            <ContractNumber type="number" use="default">${CONTRACT_NUMBER}</ContractNumber>
            <Client>
                <ClientInfo>
                    <ShortName>*{SHORT_NAME}</ShortName>
                </ClientInfo>
            </Client>
        </ContractIDT>
    </ObjectFor>
    <Data>
        <QueEvent>
            <ActionCode>${EVENT_CODE}</ActionCode>
            <Comment>${DESCRIPTION}</Comment>
        </QueEvent>
    </Data>
    <AddData>
        <SourceFileName>#{SOURCE_FILE_NAME}</SourceFileName>
    </AddData>
</Application>